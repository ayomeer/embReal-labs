#include "cppMain.h"
#include "main.h"
#include <iostream>

extern UART_HandleTypeDef huart3;

uint8_t data[] = "VCP \n\r"; // uint8_t ~ char
void testVCP(){
	HAL_UART_Transmit(&huart3, data, sizeof(data), 5);
	HAL_Delay(1000);
}

auto i=0;

void cppMain(){
	while(1){

		std::cout<<"Hello?\n\r";
		HAL_Delay(1000);
	}
}


