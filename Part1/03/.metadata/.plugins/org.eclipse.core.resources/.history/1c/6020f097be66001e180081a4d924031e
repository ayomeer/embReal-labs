#include "AccGyroSensor.h"

AccGyroSensor::AccGyroSensor() {}

AccGyroSensor::~AccGyroSensor() {}


AccGyroSensor::initSensor(){
	Gyro.init(INSTANCE, FUNCTION_ACC);
}

AccGyroSensor::updateValues(){
	Gyro.updateValues(INSTANCE, FUNCTION_GYRO);
}
